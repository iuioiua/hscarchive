{
  "lock": false,
  "imports": {
    "@std/collections": "jsr:@std/collections@^1",
    "@std/http": "jsr:@std/http@^1",
    "@std/text": "jsr:@std/text@^1",
    "preact": "npm:preact@^10",
    "preact-render-to-string": "npm:preact-render-to-string@^6"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "preact"
  },
  "tasks": {
    "serve": "deno run -A server.ts",
    "dev": "deno run -A --watch server.ts"
  }
}
